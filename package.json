{
  "name": "gzip-cli",
  "version": "1.2.0",
  "description": "gzip-cli provides CLI for compressing files by using compress algorithms such as Gzip and Brotli",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "gzip": "dist/cli.js"
  },
  "scripts": {
    "build": "webpack --env.NODE_ENV=production",
    "build:local": "webpack --env.NODE_ENV=development",
    "lint": "eslint \"{src,test}/**/*.{js,ts}\"",
    "test": "jest --maxWorkers=1 --coverage --coverageReporters=text-lcov | coveralls",
    "test:local": "jest --maxWorkers=1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pakhuta/gzip-cli.git"
  },
  "keywords": [
    "gzip-cli",
    "gzip",
    "brotli-cli",
    "brotli",
    "glob",
    "cli",
    "zip",
    "zlip",
    "compression"
  ],
  "author": "Siarhei Pakhuta",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pakhuta/gzip-cli/issues"
  },
  "homepage": "https://github.com/pakhuta/gzip-cli#readme",
  "engines": {
    "node": ">=0.12.10"
  },
  "dependencies": {
    "glob": "^7.1.6",
    "glob-parent": "^5.1.1",
    "minimist": "^1.2.5"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/glob-parent": "^5.1.0",
    "@types/jest": "^26.0.15",
    "@types/minimist": "^1.2.1",
    "@types/rimraf": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "copy-webpack-plugin": "^5.1.1",
    "coveralls": "^3.1.0",
    "eslint": "^7.14.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "jest": "^29.7.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.11",
    "tslib": "^2.0.3",
    "typescript": "^4.1.2",
    "webpack": "^5.92.0",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^2.5.2"
  }
}
